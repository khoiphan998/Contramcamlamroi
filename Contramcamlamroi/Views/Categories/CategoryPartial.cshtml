@using Contramcamlamroi.Models
@model List<Category>
<style>
    #div-u {
        margin-top: 50px;
        background-color: aquamarine;
        text-decoration: none;
       
    }

    .div-cate {
        height: 30px;
        
        
    }

    a {
        color: #a94442;
        text-decoration: none;
        font-size: 20px;
    }

        a:hover {
            text-decoration: none;
        }
</style>
<ul style="list-style-type:none;" id="div-u">
    @{
        foreach (var item in Model)
        {
            <li class="div-cate">
                @Html.ActionLink(item.NameCate, "Index", "Product", new { category = item.IDCate.Trim() }, "")
            </li>

        }
    }
</ul>
